      "url": "https://github.com/RIP21"
    }
  ],
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "esmodule": "dist/index.modern.js",
  "umd:main": "dist/index.umd.js",
  "types": "types/index.d.ts",
  "scripts": {
    "lint": "eslint ./src --ext ts,tsx --max-warnings 0",
